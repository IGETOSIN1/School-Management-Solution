@model IEnumerable<WEB_SCONET_MANAGEMENT.table_grade_range>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Result Set-Up</h1>

<h3 class="text-center">Current Configuration Set-Up for for Grade and Remarks </h3>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered table-condensed table-hover table-responsive table-striped text-center">
    <tr>
        <th class="text-center">
            From(%)
        </th>
        <th class="text-center">
            To(%)
        </th>
        <th class="text-center">
            @Html.DisplayNameFor(model => model.Grade)
        </th>
        <th class="text-center">
            @Html.DisplayNameFor(model => model.Remark)
        </th>
       
        <th class="text-center">
            @Html.DisplayNameFor(model => model.school)
        </th>
     
        <th class="text-center">
            @Html.DisplayNameFor(model => model.Date)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.From1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.To1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remark)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.school)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.p_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.p_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.p_id })
        </td>
    </tr>
}

</table>
